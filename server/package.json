    "@nestjs/core": "^11.0.1",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.2.6",
    "axios": "^1.13.5",
    "docx": "^9.6.0",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.3",
    "image-size": "^2.0.2",
    "jszip": "^3.10.1",
    "mammoth": "^1.11.0",
    "mathjax-full": "^3.2.1",
    "mathml2omml": "^0.5.0",
    "mongoose": "^9.1.3",
    "nestjs-cls": "^6.2.0",
